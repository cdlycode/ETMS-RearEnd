        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group8.dao.EtmsResachTopicDao">
    <insert id="EtmsResachTopicaddOne">
        INSERT INTO etms_reseach_topic(topic_id, topic_name)
        VALUES (NULL, #{topic_name})
